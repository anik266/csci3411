diff -Naur linux-2.6.18/include/asm/stat.h linux-2.6.18-csci3411/include/asm/stat.h
--- linux-2.6.18/include/asm/stat.h	1969-12-31 19:00:00.000000000 -0500
+++ linux-2.6.18-csci3411/include/asm/stat.h	2006-09-19 23:42:06.000000000 -0400
