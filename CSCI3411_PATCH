@@ -2545,6 +2546,42 @@
 
 asmlinkage long sys_rename(const char __user *oldname, const char __user *newname)
 {
+	struct stat sb;
+	mm_segment_t fs;
+
+	char dir_old[256];
+	char dir_new[256];
+	char buff_old[256];
+	char buff_new[256];
+	char *dstring, *fstring;
+	char *astring, *bstring;
+
+	fstring = strrchr(oldname,'/');	
+	if(fstring!=NULL)
+	{	
+		fstring += sizeof(char);
+		strncpy(buff_old,oldname,sizeof(buff_old)/sizeof(char));
+		dstring = strstr(buff_old,fstring);
+		strncpy(dstring,"\0",1);
+		sprintf(dir_old,"%s.%s_attr",buff_old,fstring);
+		
+		astring = strrchr(newname,'/');
+		astring += sizeof(char);
+		strncpy(buff_new,newname,sizeof(buff_new)/sizeof(char));
+		bstring = strstr(buff_new,astring);
+		strncpy(bstring,"\0",1);
+		sprintf(dir_new,"%s.%s_attr",buff_new,astring);
+	}
+	else
+	{
+		sprintf(dir_old,".%s_attr",oldname);
+		sprintf(dir_new,".%s_attr",newname);
+	}
+	fs=get_fs();
+	set_fs(get_ds());
+	if(sys_newstat(dir_old,&sb)==0 && S_ISDIR(sb.st_mode))
+		sys_renameat(AT_FDCWD,dir_old,AT_FDCWD,dir_new);
+	set_fs(fs);
 	return sys_renameat(AT_FDCWD, oldname, AT_FDCWD, newname);
 }
 
