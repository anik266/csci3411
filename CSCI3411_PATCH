diff -Naur linux-2.6.18/include/asm/termios.h linux-2.6.18-csci3411/include/asm/termios.h
--- linux-2.6.18/include/asm/termios.h	1969-12-31 19:00:00.000000000 -0500
+++ linux-2.6.18-csci3411/include/asm/termios.h	2006-09-19 23:42:06.000000000 -0400
