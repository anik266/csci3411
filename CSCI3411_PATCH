@@ -2529,10 +2531,9 @@
 	int error;
 	char * from;
 	char * to;
-
 	from = getname(oldname);
 	if(IS_ERR(from))
-		return PTR_ERR(from);
+		return PTR_ERR(from);	
 	to = getname(newname);
 	error = PTR_ERR(to);
